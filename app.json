{
  "expo": {
    "name": "ChinhLy",
    "slug": "ChinhLy",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/avatar.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Chinh Ly to access your photos.",
          "savePhotosPermission": "Allow Chinh Ly to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/8839eeef-4dd6-4557-8b41-25b2387b764f"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jesterjz11.ChinhLy"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.jesterjz11.ChinhLy"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "8839eeef-4dd6-4557-8b41-25b2387b764f"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
